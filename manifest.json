{
    "manifest_version": 2,

    "name": "Whats There",
    "description": "Shows nice link previews based on the page's Open Graph tags",
    "version": "1.0",

    "permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
            "all_frames": true,
            "js": [ "background.js" ],
            "css": [ "tooltip.css" ],
            "matches": [ "\u003Call_urls>" ]
        }
    ]
}
